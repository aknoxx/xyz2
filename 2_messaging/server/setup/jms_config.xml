<resources>

	<!-- DEFINE QUEUES AND TOPICS HERE -->

	<!-- EXAMPLE -->
	<admin-object-resource enabled="true"
		jndi-name="queue.dst.MyQueue" res-adapter="jmsra" res-type="javax.jms.Queue">
